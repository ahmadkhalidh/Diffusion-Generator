cat <<EOL > .gitignore
# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*$py.class

# Virtual environment
venv/
env/
.venv/

# Jupyter Notebook checkpoints
.ipynb_checkpoints

# PyTorch checkpoints
*.pt
*.pth

# Logs
logs/
*.log

# Output and model folders
outputs/
models/*.pt
/authtoken.py
# Configs
*.ini
*.yaml

# VS Code and other editor settings
.vscode/
.idea/

# OS files
.DS_Store
Thumbs.db

# Environment files
.env
.env.*

# Cache
.cache/
*.egg-info/
dist/
build/
EOL
